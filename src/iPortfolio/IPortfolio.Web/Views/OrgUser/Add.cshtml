@{
    ViewBag.Title = "OrgUser Add";
}

<ol class="breadcrumb mtFixed">
    <li><a href="~/">Home</a></li>
    <li><a href="@Url.Action("Index","OrgUser")">OrgUser</a></li>
    <li class="active">OrgUser Add</li>
</ol>

<div class="container-fluid">
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="glyphicon glyphicon-modal-window"></span>
            <span>Add OrgUser</span>
        </div>
        <div class="panel-body">
            <form id="form1">
				<div class="form-group">
					<label>UserID <span class="label label-danger">*</span></label>
					<select class="form-control" name="UserID">
						<option value="0"> == Select == </option>
					</select>
				</div>
				<div class="form-group">
					<label>OrgID <span class="label label-danger">*</span></label>
					<select class="form-control" name="OrgID">
						<option value="0"> == Select == </option>
					</select>
				</div>

				<a href="@Url.Action("Add", "OrgUser")" class="btn btn-default">
                    <span class="glyphicon glyphicon-refresh"></span>
                    <span>Refresh</span>
                </a>
                <button type="button" class="btn btn-primary" onclick="SubmitFormAsync('@Url.Action("Insert", "OrgUser")', 'form1')">Submit</button>
                <a href="@Url.Action("Index", "OrgUser")" class="btn btn-default">
                    <span class="glyphicon glyphicon-share"></span>
                    <span>Back</span>
                </a>
            </form>
        </div>
    </div>
</div>
