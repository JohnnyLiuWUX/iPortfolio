@{
    ViewBag.Title = "Audit Add";
}

<ol class="breadcrumb mtFixed">
    <li><a href="~/">Home</a></li>
    <li><a href="@Url.Action("Index","Audit")">Audit</a></li>
    <li class="active">Audit Add</li>
</ol>

<div class="container-fluid">
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="glyphicon glyphicon-modal-window"></span>
            <span>Add Audit</span>
        </div>
        <div class="panel-body">
            <form id="form1">
				<div class="form-group">
					<label>ProjectID</label>
					<input type="text" class="form-control" name="ProjectID" placeholder="Please input ProjectID">
				</div>
				<div class="form-group">
					<label>Status</label>
					<input type="text" class="form-control" name="Status" placeholder="Please input Status">
				</div>
				<div class="form-group">
					<label>DueDate</label>
					<input type="text" class="form-control" name="DueDate" placeholder="Please input DueDate">
				</div>
				<div class="form-group">
					<label>Auditor</label>
					<input type="text" class="form-control" name="Auditor" placeholder="Please input Auditor">
				</div>
				<div class="form-group">
					<label>CreatedTime</label>
					<input type="text" class="form-control" name="CreatedTime" placeholder="Please input CreatedTime">
				</div>

				<a href="@Url.Action("Add", "Audit")" class="btn btn-default">
                    <span class="glyphicon glyphicon-refresh"></span>
                    <span>Refresh</span>
                </a>
                <button type="button" class="btn btn-primary" onclick="SubmitFormAsync('@Url.Action("Insert", "Audit")', 'form1')">Submit</button>
                <a href="@Url.Action("Index", "Audit")" class="btn btn-default">
                    <span class="glyphicon glyphicon-share"></span>
                    <span>Back</span>
                </a>
            </form>
        </div>
    </div>
</div>
